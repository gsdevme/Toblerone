{
	"name": "gsdevme/Toblerone",
	"description": "Test Driven Development Tool for PHP",
	"keywords": [],
	"autoload": {
		"psr-0": {
			"": "src"
		}
	},
	"bin": [
		"src/Toblerone/bin/toblerone"
	],
	"require": {
		"symfony/console": "~2.6",
		"symfony/config": "~2.6",
		"symfony/event-dispatcher": "~2.6",
		"symfony/dependency-injection": "~2.6",
		"phpunit/phpunit": "~4.6",
		"phpunit/php-code-coverage": "~2.0"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "~2.3"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "develop"
		}
	}
}
